{
    'parser': 'babel-eslint',
    'rules': {
        'indent': [2, 4],
        'quotes': [2, 'single'],
        'semi': [2, 'always'],
        'comma-style': [2, 'last']
    },
    'ecmaFeatures': {
        'modules': true
    },
    'env': {
        'es6': true,
        'node': true
    },
    'extends': 'eslint:recommended'
}
